{
  "src": {
  	"base": "src/",
  	"css": "src/css/**/*",
    "html": "src/**.html",
  	"htmlWatchFiles": [
      "src/**.html",
      "src/md/**.md",
      "src/includes/**/*.html"
    ],
		"includes": "src/includes",
    "includeJson": "src/includes/json/process-trello.json",    
  	"images": "src/images/**/*",
    "jpgs": "src/images/**/*.jpg",
  	"js": "src/js/**/*",
    "metaFiles": [
      "src/.htaccess",
      "src/chrisgurney-resume.pdf",
      "src/favicon.png",
      "src/manifest.json",
      "src/pinterest-7fcd1.html",
      "src/robots.txt",
      "src/sw.js"
    ],
 		"vendor": "src/vendor/**/*"
  },
  "output": {
   	"base": "dist/",
   	"css": "dist/css",
  	"html": "dist/",
  	"images": "dist/images/",
  	"js": "dist/js/",  	
    "includeJson": "dist/process.json",    
